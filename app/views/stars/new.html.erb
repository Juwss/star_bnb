<h1 class="text-align">Ajouter une star</h1>

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name),
  data: {turbo: false}) do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </div>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>

   <div class="actions">
    <%= f.submit "Log out" %>
  </div>
<% end %>
